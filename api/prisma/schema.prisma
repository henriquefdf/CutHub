

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id       Int      @id @default(autoincrement())
  nome    String
  email   String    @unique
  tipo    String
  senha   String
  foto    String? //URL da foto do usu√°rio
  tokenRecPass String?    @unique @db.VarChar(255)
  dateRecPass  DateTime?  @db.DateTime(3)
}
